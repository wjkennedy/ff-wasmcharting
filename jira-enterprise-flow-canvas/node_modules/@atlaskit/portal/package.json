{
	"name": "@atlaskit/portal",
	"version": "5.2.1",
	"description": "A wrapper for rendering components in React portals.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Portal",
			"category": "Libraries"
		}
	},
	"dependencies": {
		"@atlaskit/app-provider": "^4.0.0",
		"@atlaskit/platform-feature-flags": "^1.1.0",
		"@atlaskit/theme": "^21.0.0",
		"@babel/runtime": "^7.0.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/button": "^23.9.0",
		"@atlaskit/code": "^17.4.0",
		"@atlaskit/css": "^0.19.0",
		"@atlaskit/docs": "^11.3.0",
		"@atlaskit/flag": "^17.8.0",
		"@atlaskit/icon": "^30.0.0",
		"@atlaskit/inline-dialog": "^18.0.0",
		"@atlaskit/link": "^3.3.0",
		"@atlaskit/modal-dialog": "^14.10.0",
		"@atlaskit/onboarding": "^14.5.0",
		"@atlaskit/primitives": "^18.0.0",
		"@atlaskit/section-message": "^8.12.0",
		"@atlaskit/tokens": "^11.0.0",
		"@atlaskit/tooltip": "^20.14.0",
		"@atlassian/feature-flags-test-utils": "^1.0.0",
		"@atlassian/ssr-tests": "workspace:^",
		"@compiled/react": "^0.18.6",
		"@testing-library/react": "^16.3.0",
		"bind-event-listener": "^3.0.0",
		"raf-stub": "^2.0.1"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"compiled"
			]
		}
	},
	"homepage": "https://atlassian.design/components/portal/",
	"platform-feature-flags": {
		"platform_design_system_team_portal_logic_r18_fix": {
			"type": "boolean"
		},
		"platform_dst_subtree_theming": {
			"type": "boolean"
		}
	}
}
