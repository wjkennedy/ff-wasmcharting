			"design-system": "v1",
			"dom-events": "use-bind-event-listener",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"theming": [
				"react-context"
			],
			"styling": [
				"static",
				"emotion"
			],
			"deprecation": "no-deprecated-imports"
		}
	},
	"typesVersions": {
		">=4.5 <4.9": {
