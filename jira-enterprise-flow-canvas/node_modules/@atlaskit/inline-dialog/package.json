{
	"name": "@atlaskit/inline-dialog",
	"version": "18.0.9",
	"description": "An inline dialog is a pop-up container for small amounts of information. It can also contain controls.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Inline dialog",
			"status": {
				"type": "intent-to-deprecate",
				"description": "We are planning on deprecating Inline dialog. We recommend using the Popup component instead.",
				"actions": [
					{
						"text": "View Popup documentation",
						"href": "https://atlassian.design/components/popup/examples"
					}
				]
			},
			"category": "Overlays and layering"
		}
	},
	"dependencies": {
		"@atlaskit/analytics-next": "^11.1.0",
		"@atlaskit/ds-lib": "^5.3.0",
		"@atlaskit/layering": "^3.6.0",
		"@atlaskit/popper": "^7.1.0",
		"@atlaskit/theme": "^21.0.0",
		"@atlaskit/tokens": "^11.0.0",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.6",
		"bind-event-listener": "^3.0.0"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/button": "^23.9.0",
		"@atlaskit/css": "^0.19.0",
		"@atlaskit/datetime-picker": "^17.4.0",
		"@atlaskit/docs": "^11.3.0",
		"@atlaskit/dropdown-menu": "^16.4.0",
		"@atlaskit/link": "^3.3.0",
		"@atlaskit/modal-dialog": "^14.10.0",
		"@atlaskit/primitives": "^18.0.0",
		"@atlaskit/section-message": "^8.12.0",
		"@atlaskit/select": "^21.7.0",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "^16.3.0",
		"react-dom": "^18.2.0",
		"react-lorem-component": "^0.13.0"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"theming": [
				"react-context"
			],
			"styling": [
				"static",
				"emotion"
			],
			"design-system": "v1",
			"deprecation": "no-deprecated-imports"
		}
	},
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"platform-feature-flags": {
		"dst-a11y__replace-anchor-with-link__design-system-": {
			"type": "boolean"
		}
	},
	"homepage": "https://atlassian.design/components/inline-dialog/"
}
